	"main": "build/index.js",
	"dependencies": {
		"@wordpress/block-editor": "^6.1.0",
		"@wordpress/blocks": "^9.1.0",
		"@wordpress/element": "^3.1.0",
		"@wordpress/i18n": "^4.1.0",
		"classnames": "^2.3.1"
	},
	"devDependencies": {
		"@wordpress/scripts": "^18.1.0"
	}
}
