{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cata/network-link",
	"version": "0.1.0",
	"title": "Network Link",
	"category": "widgets",
	"parent": [ "cata/network-links" ],
	"description": "Display a logo linking to another site in the Thought Catalog network.",
	"supports": {
		"html": false,
		"reusable": false
	},
	"attributes": {
		"url": {
			"type": "string"
		},
		"service": {
			"type": "string"
		},
		"width": {
			"type": "string"
		}
	},
	"example": {
		"attributes": {
			"service": "thoughtcatalog",
			"url": "https://thoughtcatalog.com"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
