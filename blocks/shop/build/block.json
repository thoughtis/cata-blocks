{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cata/shop",
	"version": "0.1.0",
	"title": "Shop",
	"category": "widgets",
	"icon": "smiley",
	"description": "This block produces a block of products pulled from Shop Catalog.",
	"supports": {
		"html": false,
		"align": true
	},
	"attributes": {
		"shopCatalogApiUrlBase": {
			"type": "string",
			"default": "https://shopcatalog.com/wp-json/wc/v3/products/"
		},
		"category": {
			"type": "string",
			"default": "1752"
		},
		"per_page": {
			"type": "string",
			"default": "6"
		},
		"orderby": {
			"type": "string",
			"default": "menu_order"
		},
		"order": {
			"type": "string",
			"default": "asc"
		},
		"results": {
			"type": "object",
			"default": {}
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
