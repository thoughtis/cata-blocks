{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cata/reel",
	"title": "Reel",
	"category": "design",
	"description": "A container that allows the user to scroll horizontally to view additional content.",
	"keywords": [ "slider", "scroll" ],
	"icon": "slides",
	"supports": {
		"align": ["wide","full"],
		"className": true,
		"html": false,
		"spacing": {
			"margin": true
		}
	},
	"variations": [
		{
			"name": "auto-slider",
			"title": "Auto Slider",
			"attributes": {
				"className": "is-style-auto-slider"
			}
		}
	],
	"textdomain": "cata",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}