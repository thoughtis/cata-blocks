{
  "apiVersion": 2,
  "name": "cata/newsletter",
  "version": "0.1.0",
  "title": "Newsletter Signup Form",
  "category": "widgets",
  "icon": "feedback",
  "description": "This widget places a mailchimp newsletter signup form on the page wherever it is used.",
  "supports": {
    "html": false,
    "multiple": false,
    "customClassName": false,
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": true,
        "radius": true,
        "style": true,
        "width": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "classNameBase": {
      "type": "string",
      "default": "wp-block-cata-newsletter"
    },
    "mailchimpUrlBase": {
      "type": "string",
      "default": "https://thoughtcatalog.us2.list-manage.com/subscribe/post?u="
    },
    "mailchimpUserId": {
      "type": "string",
      "default": "ce27c7f9ab856e12c6f2bf1da"
    },
    "mailchimpUrlAmp": {
      "type": "string",
      "default": "&amp;id="
    },
    "mailchimpAudienceId": {
      "type": "string",
      "default": ""
    },
    "legalText": {
      "type": "string",
      "default": "Unsubscribe at any time. By subscribing, you agree to the terms of our %%Privacy Policy%%."
    },
    "successMessage": {
      "type": "string",
      "default": "Thanks for subscribing!"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "script": "file:./script.js"
}