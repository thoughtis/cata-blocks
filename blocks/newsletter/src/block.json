{
	"apiVersion": 2,
	"name": "cata/newsletter",
	"version": "0.1.0",
	"title": "Newsletter Signup Form",
	"category": "widgets",
	"icon": "feedback",
	"description": "This widget places a mailchimp newsletter signup form on the page wherever it is used.",
	"supports": {
		"html": false,
		"multiple": false,
		"customClassName": false,
		"align": true
	},
	"attributes": {
		"classNameBase": {
			"type": "string",
			"default": "wp-block-cata-newsletter"
		},
		"mailchimpUrlBase": {
			"type": "string",
			"default": "https://thoughtcatalog.us2.list-manage.com/subscribe/post?u="
		},
		"mailchimpUserId": {
			"type": "string",
			"default": "ce27c7f9ab856e12c6f2bf1da"
		},
		"mailchimpUrlAmp": {
			"type": "string",
			"default": "&amp;id="
		},
		"mailchimpAudienceId": {
			"type": "string",
			"default": "829754e1b3"
		},
		"legalText": {
			"type": "string",
			"default": "Unsubscribe at any time. By subscribing, you agree to the terms of our %%Privacy Policy%%"
		},
		"successMessage": {
			"type": "string",
			"default": "Thanks and stay spooky!"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"script": "file:./script.js"
}
