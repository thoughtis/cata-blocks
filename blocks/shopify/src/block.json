{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "cata/shopify",
	"version": "0.1.0",
	"title": "Shopify Products",
	"category": "widgets",
	"icon": "store",
	"description": "Display Shopify products as obtained from the Shopify Storefront API",
	"supports": {
		"align": ["wide","full"],
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"store": {
			"type": "string",
			"default": ""
		},
		"category": {
			"type": "string",
			"default": ""
		},
		"product_cat_exclude": {
			"type": "string",
			"default": ""
		},
		"coauthor": {
			"type": "string",
			"default": ""
		},
		"tag": {
			"type": "string",
			"default": ""
		},
		"per_page": {
			"type": "string",
			"default": "6"
		},
		"orderby": {
			"type": "string",
			"default": "menu_order"
		},
		"order": {
			"type": "string",
			"default": "asc"
		},
		"query_url": {
			"type": "string",
			"default": ""
		},
		"display_byline": {
			"type": "boolean",
			"default": true
		},
		"display_price": {
			"type": "boolean",
			"default": true
		}
	},
	"keywords": [ "shop", "shopify", "store", "product" ],
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}